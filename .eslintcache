LE) {\n    return <ReviewsList />;\n  }\n\n  if (status === Status.PENDING) {\n    return <Spinner />;\n  }\n\n  if (status === Status.REJECTED) {\n    return <div>{error.message}</div>;\n  }\n\n  if (status === Status.RESOLVED) {\n    return <ReviewsList reviews={reviews} />;\n  }\n}\n","/Users/viktoriiamelanchenko/Documents/GitHub/goit-react-hw-04-movies1/src/components/Navigation/Navigation.jsx",[],"/Users/viktoriiamelanchenko/Documents/GitHub/goit-react-hw-04-movies1/src/components/FetchMovieRequest/FetchMovieRequest.jsx",["81"],"import { useState, useEffect } from 'react';\nimport { fetchSearchMovie } from '../../service/api_movie';\nimport Spinner from '../Loader/Loader';\nimport MovieCard from '../MovieCard/MovieCard';\nimport PropTypes from 'prop-types';\n\nconst Status = {\n  IDLE: 'idle',\n  PENDING: 'pending',\n  RESOLVED: 'resolved',\n  REJECTED: 'rejected',\n};\n\nexport default function FetchMovieRequest({ query }) {\n  const [movies, setMovies] = useState([]);\n  const [status, setStatus] = useState(Status.IDLE);\n  const [error, setError] = useState(null);\n\n  useEffect(() => {\n    if (!query) {\n      return;\n    }\n\n    setStatus(Status.PENDING);\n    fetchMovies();\n    setStatus(Status.RESOLVED);\n  }, [query]);\n\n  const fetchMovies = () => {\n    fetchSearchMovie(query)\n      .then(({ results }) => {\n        setMovies(results);\n        if (!results.length) {\n          throw new Error('Hmm...Nothing here. Try another search.');\n        }\n      })\n      .catch((error) => {\n        setError(error);\n        setStatus(Status.REJECTED);\n      });\n  };\n\n  if (status === Status.IDLE) {\n    return (\n      <div style={{ textAlign: 'left' }}>\n        Please enter a search term to begin your search...\n      </div>\n    );\n  }\n\n  if (status === Status.PENDING) {\n    return <Spinner />;\n  }\n\n  if (status === Status.REJECTED) {\n    return <div>{error.message}</div>;\n  }\n\n  if (status === Status.RESOLVED) {\n    return <>{movies && <MovieCard movies={movies} />}</>;\n  }\n}\n\nFetchMovieRequest.propTypes = {\n  query: PropTypes.string.isRequired,\n};\n","/Users/viktoriiamelanchenko/Documents/GitHub/goit-react-hw-04-movies1/src/components/Cast/Cast.jsx",["82"],"import { useState, useEffect } from 'react';\nimport { fetchMovieCredits } from '../../service/api_movie';\nimport Cast from '../CastItem/CastItem';\nimport Spinner from '../Loader/Loader';\n\nconst Status = {\n  IDLE: 'idle',\n  PENDING: 'pending',\n  RESOLVED: 'resolved',\n  REJECTED: 'rejected',\n};\n\nexport default function CastView({ movieId }) {\n  const [characters, setCharacters] = useState([]);\n  const [status, setStatus] = useState(Status.IDLE);\n  const [error, setError] = useState(null);\n\n  useEffect(() => {\n    setStatus(Status.PENDING);\n    getCharacters();\n    setStatus(Status.RESOLVED);\n  }, [movieId]);\n\n  const getCharacters = () => {\n    fetchMovieCredits(movieId)\n      .then((request) => setCharacters(request.cast))\n      .catch((error) => {\n        setError(error);\n        setStatus(Status.REJECTED);\n      });\n  };\n\n  if (status === Status.IDLE) {\n    return <Cast />;\n  }\n\n  if (status === Status.PENDING) {\n    return <Spinner />;\n  }\n\n  if (status === Status.REJECTED) {\n    return <div>{error.message}</div>;\n  }\n\n  if (status === Status.RESOLVED) {\n    return <Cast characters={characters} />;\n  }\n}\n","/Users/viktoriiamelanchenko/Documents/GitHub/goit-react-hw-04-movies1/src/components/ReviewsList/ReviewsList.jsx",[],"/Users/viktoriiamelanchenko/Documents/GitHub/goit-react-hw-04-movies1/src/components/MovieCard/MovieCard.jsx",[],"/Users/viktoriiamelanchenko/Documents/GitHub/goit-react-hw-04-movies1/src/components/CastItem/CastItem.jsx",[],{"ruleId":"83","replacedBy":"84"},{"ruleId":"85","replacedBy":"86"},{"ruleId":"87","severity":1,"message":"88","line":23,"column":6,"nodeType":"89","endLine":23,"endColumn":15,"suggestions":"90"},{"ruleId":"87","severity":1,"message":"91","line